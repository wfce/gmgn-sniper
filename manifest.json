{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1.1",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "permissions": ["storage"],
  "host_permissions": ["https://gmgn.ai/*"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://gmgn.ai/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}